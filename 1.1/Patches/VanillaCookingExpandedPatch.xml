<?xml version="1.0" encoding="utf-8" ?>
<Patch>


	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Cooking Expanded</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>


				



				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/HediffDef[defName = "VCE_WellFedFine"]/comps/li/removeThoughtDefs</xpath>
					<value>

						<li>VCE_AteFineSushi</li>

					</value>
				</li>
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/HediffDef[defName = "VCE_WellFedLavish"]/comps/li/removeThoughtDefs</xpath>
					<value>

						<li>VCE_AteLavishSushi</li>

					</value>
				</li>
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/HediffDef[defName = "VCE_WellFedGourmet"]/comps/li/removeThoughtDefs</xpath>
					<value>

						<li>VCE_AteGourmetSushi</li>

					</value>
				</li>
				<li Class="PatchOperationAdd"> 
					<xpath>/Defs/RecipeDef[defName = "VCE_DeepFryMeats"]/defaultIngredientFilter/disallowedThingDefs</xpath>
					<value>

						<li>VCE_DeepFriedSushi</li>

					</value>
				</li>
				

			</operations>
		</match>
	</Operation>


</Patch>

