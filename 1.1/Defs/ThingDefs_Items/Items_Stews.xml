<?xml version="1.0" encoding="utf-8" ?>
<Defs>  


	<ThingDef ParentName="RoughPlantBase">
		<defName>VCE_UncookedStewSimple</defName>
		<label>uncooked simple stew</label>
		<description>An uncooked simple stew. This needs to be cooked in a stew pot to be edible.</description>
		<graphicData>
			<texPath>Things/Items/UncookedStew/VCE_UncookedStewSimple</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>120</MarketValue>
			<Flammability>1.3</Flammability>
			<WorkToMake>300</WorkToMake>
		</statBases>
		<tradeability>None</tradeability>
		<comps>
			<li Class="CompProperties_Rottable">
				<daysToRotStart>20</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
			<li Class="CompProperties_Ingredients" />			
		</comps>
		<thingCategories>
			<li>VCE_UncookedStews</li>
		</thingCategories>
		<ingestible>
			<preferability>NeverForNutrition</preferability>			
		</ingestible>
		
	</ThingDef>
	
	<ThingDef ParentName="MealCooked">
		<defName>VCE_CookedStewSimple</defName>
		<label>simple stew</label>
		<description>A simple stew slow cooked from a few ingredients.</description>
		<graphicData>
			<texPath>Things/Meals/VCE_StewSimple</texPath>
			<graphicClass>Graphic_StackCount</graphicClass>
		</graphicData>
		<statBases>
			<MarketValue>14</MarketValue>
			<Nutrition>0.8</Nutrition>
		</statBases>
		<ingestible>
			<preferability>MealSimple</preferability>
			<ingestEffect>EatVegetarian</ingestEffect>
			<ingestSound>Ingest_Drink</ingestSound>
		</ingestible>

	</ThingDef>
	
